#######################
MENETHIL CORE: WORLD SERVER CONFIG
#######################

#######################
SECTION: Logger
#######################
[logger]
LogLevelFormat = "| %-6s|"
LogMessageFormat = "%s
LogFieldNameFormat = "%s:"
LogFieldValueFormat = "%s"

#######################
SECTION: SERVER
#######################
[server]
ListenHost = "0.0.0.0"
ListenPort = 8085

#######################
SECTION: DATABASE
#######################
[database]
DatabaseDriver = "postgres"
DataaseHost = "127.0.0.1"
DatabasePort = 5432
DatabaseUsername = ""
DatabasePassowrd = ""
DatabaseWorldDatabase = "menethil_world"
